{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:@typescript-eslint/recommended",
        "eslint:recommended"
    ],

    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        " @typescript-eslint/eslint-plugin"
    ],
    "rules": {
        "semi": ["error", "always"],
        "@typescript-eslint/no-explicit-any": "off"
    }
}
